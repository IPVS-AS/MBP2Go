<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">


    <com.highsoft.highcharts.Core.HIChartView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="292dp"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/minValue"
        android:layout_width="50dp"
        android:layout_height="20sp"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/maxValue"
        android:layout_width="50dp"
        android:layout_height="20sp"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/t"
        android:layout_width="50dp"
        android:layout_height="20sp"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:text="TextView"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView10"
        android:layout_alignTop="@+id/textView6"
        android:layout_marginTop="15dp"
        android:text="Minimal Value:" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/webView"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:text="Maximal Value:" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_alignTop="@+id/textView11"
        android:layout_marginTop="15dp"
        android:text="Average Value:" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/maxValue"
        android:layout_marginTop="10dp"
        android:background="#eeee" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="10dp"
        android:background="#eeee" />

    <ImageButton
        android:id="@+id/btn_History_actual"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginBottom="60dp"
        android:background="@drawable/bg_form_rounded_lila"
        android:src="@drawable/ic_baseline_sync_24px" />




</RelativeLayout>